    if n == 0 then
        r
    else
        let
            normalize x max = Prelude.fromInt x * (max / imd);

            newseed = Prelude.remainder (seed * ia + ic) im;

            newrand = normalize newseed max;

            im = 139968;

            imd = Prelude.fromInt im;

            ia = 3877;

            ic = 29573;
        in
            randloop (n - 1) newseed newrand max
    ;

