f1 :: Prelude.Integer -> Prelude.Integer;
f1 !x =
    if x < 2 then
        1
    else
        x * f1 (x - 1)
    ;

f2 :: Prelude.Integer -> Prelude.Integer;
f2 !x =
    if x < 2 then
        1
    else
        f2 (x - 1) * x
    ;

