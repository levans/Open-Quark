        groupAllByHelper eq !list equivClassesOfLists =
            case list of
            [] -> equivClassesOfLists;
            listHead : listTail ->
                groupAllByHelper
                    eq
                    listTail
                    (groupValue eq listHead equivClassesOfLists)
