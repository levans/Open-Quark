facAcc :: Prelude.Num a => a -> a -> a;
private facAcc a n =
    if n == 0 then
        a
    else
        facAcc (n * a) (n - 1)
    ;

factorial :: Prelude.Num a => a -> a;
factorial = facAcc 1;

