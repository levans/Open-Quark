        helper :: Int -> [Int] -> [Int];
        helper !index !currentList =
            if index <= n then
                helper
                    (index + 1)
                    (Prelude.deepStrict
                        (map (Prelude.add index))
                        currentList
                    )
            else
                currentList
            ;
    in
        List.sum (helper 1 list)
    ;

