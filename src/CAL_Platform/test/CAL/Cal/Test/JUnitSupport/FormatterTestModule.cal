        groupValue :: (a -> a -> Boolean) -> a -> [[a]] -> [[a]];
        groupValue eq value !list =
            case list of
            [] -> [[value]];
            listHead : listTail ->
                if eq value (head listHead) then
                    (value : listHead) : listTail
                else
                    listHead : groupValue eq value listTail
                ;
            ;
    in
        groupAllByHelper eqFunc list []
