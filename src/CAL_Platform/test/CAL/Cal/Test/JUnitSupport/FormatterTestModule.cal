maxBoundAltMaybe :: BoundedAlt a => Maybe a;
maxBoundAltMaybe = Just maxBoundAlt;

instance ShowAlt a => ShowAlt (Maybe a) where
    showAlt = showAltMaybe;
    ;

showAltMaybe :: ShowAlt a => Maybe a -> String;
showAltMaybe !m =
    case m of
    Nothing -> "Nothing";
    Just x -> "(Just " ++ showAlt x ++ ")";
