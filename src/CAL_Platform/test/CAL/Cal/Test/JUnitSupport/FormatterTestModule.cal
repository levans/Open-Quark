testLet2 :: Int;
testLet2 =
    let
        mySum xs =
            case xs of
            [] -> 0;
            b : bs -> b + mySum bs;
            ;

        myProd xs =
            case xs of
            [] -> 1;
            b : bs -> b * myProd bs;
            ;

        oneToFour = List.take 4 (Prelude.upFrom 1);
    in
        mySum oneToFour + myProd oneToFour
    ;

