lifter :: Int -> Int;
lifter !x =
    let
        f y =
            let
                g z =
                    let
                        h w = x + 2 * y + 3 * z + 4 * w;
                    in
                        h (z + 3)
                    ;
            in
                g (y + 2)
            ;
    in
        f (x + 1)
    ;

lifter2 :: Int -> Int -> Int;
lifter2 n w =
    let
        f x =
            let
                g y =
                    let
                        h z =
                            if z == 0 then
                                0
                            else
                                f (x - w) + z
                            ;
                    in
                        h y
                    ;
            in
                g x
            ;
    in
        f n
    ;

