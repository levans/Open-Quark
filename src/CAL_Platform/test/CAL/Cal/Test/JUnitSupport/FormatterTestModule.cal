        groupValue eqFunc groups value !remainingGroups =
            case remainingGroups of
            [] -> [value] : groups;
            x : xs ->
                if eqFunc value (head x) then
                    (value : x) : xs
                else
                    x : groupValue eqFunc groups value xs
